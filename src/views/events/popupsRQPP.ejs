<!---------reserve quota popup---------->
<div class="popup" id="rqpp">
    
    <div class="popupContent" id="rqppContent">

        <div class="popupIcons">
            <div class="popupIcon1"><i class="fa-regular fa-pen-to-square"></i></div>
            <div class="popupClose1" id="rqppClose"><i class="fa-solid fa-rectangle-xmark popupCloseIcon"></i></div>
        </div>

        <div class="popupTitle1 mtm" id="rqppMainTitle"></div>
        <div class="popupTitle1 mts" id="rqppCourse"></div>
        <div class="popupTitle2 mtm" id="rqppDate"></div>
        <div class="popupTitle2 mts" id="rqppTime"></div>
        <div class="popupTitle2 mtm" id="rqppAvailableQuota"></div>
        
        <div class="divInputsColumn mtm">
            <div class="divInput" id="rqppDivCompany">
                <label class="label3" id="rqppCompanyLabel">Empresa</label>
                <select class="input1 ixxl ipl" id="rqppCompany"></select>
            </div>
            <div class="divError" id="rqppCompanyError"></div>
            <div class="divInput mtm">
                <label class="label3" id="rqppQuotaLabel">Cupos a reservar</label>
                <input type="number" class="input1 im ipl" id="rqppQuota">                
            </div>
            <div class="divError" id="rqppQuotaError"></div>
        </div>

        <div class="divButtonsRow mtl mbs">
            <div id="rqppAccept" class="button1 bs"></div>
            <div id="rqppCancel" class="button1 bs">Cancelar</div>
        </div>

    </div>
</div>

<div class="okPopup" id="rqppOk">    
    <div class="okContent" id="rqppOkContent">
        <div class="okDiv">
            <div class="okIcon1"><i class="fa-regular fa-circle-check"></i></div>
            <div class="okText" id="rqppOkText"></div>
        </div>
    </div>
</div>

<div class="errorPopup" id="rqppError">    
    <div class="errorContent" id="rqppErrorContent">
        <div class="okDiv">
            <div class="okIcon1"><i class="fa-solid fa-triangle-exclamation"></i></div>
            <div class="okText" id="rqppErrorText"></div>
        </div>
    </div>
</div>
