<!----------upload excel popup---------->
<div class="popup2" id="uepp">
    
    <div class="popupContent" id="ueppContent">

        <div class="popupIcons">
            <div class="popupIcon1"><i class="fa-regular fa-file-excel"></i></div>
            <div class="popupClose1" id="ueppClose"><i class="fa-solid fa-rectangle-xmark popupCloseIcon"></i></div>
        </div>

        <div id="ueppDownloadTemplate">                
            <i class="fa-solid fa-download" id="ueppDownloadTemplateIcon"></i>
            <div id="showActionTemplate">
                Descargar template
            </div>
        </div>

        <div class="popupTitle1">SUBIR ARCHIVO</div>
        
        <div id="ueppDivInput" class="mtl">
            <input type="file" id="ueppFile" name="ueppFile">                        
        </div>
        <div id="ueppFileError" class="divError"></div>
        
        <div class="divButtonsRow mtl mbs">
            <div id="ueppAccept" class="button1 bs">Aceptar</div>
            <div id="ueppCancel" class="button1 bs">Cancelar</div>
        </div>
    </div>
</div>