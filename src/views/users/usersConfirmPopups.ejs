<!----------Block user popup---------->
<div class="popup" id="bupp">
    
    <div class="popupContent" id="buppContent">

        <div class="popupIcons">
            <div class="popupIcon1"><i class="fa-solid fa-ban"></i></div>
            <div class="popupClose1" id="buppClose"><i class="fa-solid fa-rectangle-xmark popupCloseIcon"></i></div>
        </div>

        <div class="popupQuestion" id="buppQuestion"></div>       
        
        <div class="divButtonsRow mbs">
            <div id="buppAccept" class="button1 bs">Aceptar</div>
            <div id="buppCancel" class="button1 bs">Cancelar</div>
        </div>
    </div>
</div>

<div class="okPopup" id="buppOk">    
    <div class="okContent" id="buppOkContent">
        <div class="okDiv">
            <div class="okIcon1"><i class="fa-regular fa-circle-check"></i></div>
            <div class="okText">Usuario dado de baja con éxito</div>
        </div>
    </div>
</div>

